# Environment variables for Core Staking ETF
# Copy this file to .env and fill in your actual values
# NEVER commit real keys to version control

# WalletConnect Project ID (get from https://cloud.walletconnect.com/)
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Core Network Configuration
VITE_CORE_MAINNET_RPC=https://rpc.coredao.org/
VITE_CORE_TESTNET_RPC=https://rpc.test2.btcs.network/

# Private keys for deployment (NEVER commit real keys)
PRIVATE_KEY=your_private_key_here
DEPLOYER_PRIVATE_KEY=your_deployer_private_key_here

# Core API Keys (get from Core DAO documentation)
CORE_API_KEY=your_core_api_key_here
VITE_CORE_API_KEY=your_core_api_key_here
VITE_CORE_TEST2_BTCS_KEY=your_core_test_api_key_here

# Frontend Configuration  
VITE_BACKEND_URL=http://localhost:3001

# Backend Configuration
NODE_ENV=development
PORT=3001

# Database Configuration (if using external database)
DATABASE_URL=your_database_url_here

# Security Configuration
JWT_SECRET=your_jwt_secret_here
API_SECRET_KEY=your_api_secret_key_here